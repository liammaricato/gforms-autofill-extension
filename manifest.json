{
  "manifest_version": 3,
  "version": "1.0",
  "name": "GoogleFormsAutofill",
  "content_scripts": [
    {
      "matches": ["https://*.google.com/forms/*"],
      "css": ["style.css"],
      "js": ["main.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
